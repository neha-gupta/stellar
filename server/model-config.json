{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ApplicationUser": {
    "dataSource": "StellarAuthDB",
    "options": {
      "strictObjectIDCoercion": false
    },
    "public" : true
  },
  "ApplicationAccessToken": {
    "dataSource": "StellarAuthDB",
    "public": false
  },
  "ACL": {
    "dataSource": "StellarAuthDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "StellarAuthDB",
    "public": false
    
  },
  "Role": {
    "dataSource": "StellarAuthDB",
    "public": false
  },
  "ProjectConfiguration": {
    "dataSource": "StellarDB",
    "public": true,
    "options": {
      "validateUpsert": true,
      "mongodb": {
      "allowExtendedOperators": true
      }
    }
},
  "Metrics": {
    "dataSource": "StellarDB",
    "public": true,
    "options": {
      "validateUpsert": true,
      "mongodb": {
      "allowExtendedOperators": true
      }
    }
  }
}
