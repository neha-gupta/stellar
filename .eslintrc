{
  "parser": "babel-eslint",
  "plugins": [
      "react"
  ],
  "env":
  {
      "browser": true,
      "node": true,
      "es6": true
  },
  "rules":
  {
      "max-len": [1, 300, 2,
      {
          "ignoreComments": true
      }],
      "react/prop-types": [2],
      "no-console": 0,
      "no-mixed-spaces-and-tabs": [0, "smart-tabs"],
      "complexity": ["error", 26]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}